{
  "$schema": "src/Doki.CommandLine/doki.config.schema.json",
  "input": [
    "src/**/*.csproj",
    "!src/Doki.CommandLine/Doki.CommandLine.csproj"
  ],
  "output": [
    {
      "type": "Doki.Output.Markdown",
      "from": "src/Doki.Output.Markdown/Doki.Output.Markdown.csproj",
      "options": {
        "outputPath": "docs/api"
      }
    },
    {
      "type": "Doki.Output.ClassLibrary",
      "from": "src/Doki.Output.ClassLibrary/Doki.Output.ClassLibrary.csproj",
      "options": {
        "outputPath": "src/Doki.Documentation.Content",
        "namespace": "Doki.Documentation.Content"
      }
    }
  ]
}